journal = {Ambio},
keywords = {Antarctic Peninsula,Chinstrap penguin,Gentoo penguin,Population growth rate,Southern annular mode},
mendeley-groups = {DOCAS,DOCAS/Ensayo{\_}CC,DOCAS/Proyecto{\_}Tesis{\_}DOCAS,DOCAS/SA{\_}Krill},
number = {3},
pages = {560--571},
